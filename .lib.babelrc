{
  "presets": [
    ["@babel/env", {
      "targets": {
        "node": "6.4.0",
        "browsers": [ "> 1%", "last 2 versions", "ie 9" ]
      }
    }]
  ],
  "plugins": [
    ["@babel/transform-runtime"]
  ],
  "parserOpts": {
    "allowReturnOutsideFunction": true
  },
  "sourceMaps": "inline"
}
